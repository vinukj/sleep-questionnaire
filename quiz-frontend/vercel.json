{
  "rewrites": [
    { "source": "/auth/:path*", "destination": "http://194.238.16.106:5000/auth/:path*" },
    { "source": "/api/:path*", "destination": "http://194.238.16.106:5000/api/:path*" },
    { "source": "/questionnaire/:path*", "destination": "http://194.238.16.106:5000/questionnaire/:path*" },
    { "source": "/ocr/:path*", "destination": "http://194.238.16.106:5000/ocr/:path*" },
    { "source": "/about/:path*", "destination": "http://194.238.16.106:5000/about/:path*" },
    { "source": "/quizzes/:path*", "destination": "http://194.238.16.106:5000/quizzes/:path*" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cross-Origin-Opener-Policy", "value": "same-origin-allow-popups" },
        { "key": "Cross-Origin-Embedder-Policy", "value": "unsafe-none" }
      ]
    }
  ]
}
